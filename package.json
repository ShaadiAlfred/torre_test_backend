{
  "name": "torre_test_backend",
  "version": "1.0.0",
  "description": "Backend for Torre's test",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "npx nodemon src/index.ts",
    "build": "npx tsc",
    "jasmine": "jasmine --config=spec/support/jasmine.json",
    "test": "cross-env NODE_ENV=test npm run build && cross-env NODE_ENV=test npm run jasmine",
    "lint": "eslint src/*/**/*.ts",
    "lintf": "eslint src/*/**/*.ts --fix",
    "prettier": "npx prettier --write . --ignore-path .gitignore"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.2",
    "@types/node": "^16.11.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "express": "^4.17.1"
  }
}
